	<queryString language="SQL">
		<![CDATA[SELECT
     cliente.`id` AS cliente_id,
     cliente.`nombre` AS cliente_nombre,
     cliente.`apellido` AS cliente_apellido,
     cliente.`telefono` AS cliente_telefono,
     cliente.`direccion` AS cliente_direccion,
     factura.`fecha` AS factura_fecha,
     factura.`id_usuario` AS factura_id_usuario,
     factura.`id_factura` AS factura_id_factura,
     factura.`id_cliente` AS factura_id_cliente,
     factura_has_producto.`factura_id_factura` AS factura_has_producto_factura_id_factura,
     factura_has_producto.`producto_id_producto` AS factura_has_producto_producto_id_producto,
     factura_has_producto.`cantidad` AS factura_has_producto_cantidad,
     gestiondeusuarios.`id_usuario` AS gestiondeusuarios_id_usuario,
     gestiondeusuarios.`nombre` AS gestiondeusuarios_nombre,
     producto.`id_producto` AS producto_id_producto,
     producto.`tipo` AS producto_tipo,
     producto.`gama` AS producto_gama,
     producto.`referencia` AS producto_referencia,
     producto.`valor` AS producto_valor,
     producto.`existencia` AS producto_existencia,
     producto.`marca` AS producto_marca,
     producto.`detalle` AS producto_detalle,
     gestiondeusuarios.`apellido` AS gestiondeusuarios_apellido
FROM
     `cliente` cliente INNER JOIN `factura` factura ON cliente.`id` = factura.`id_cliente`
     INNER JOIN `factura_has_producto` factura_has_producto ON factura.`id_factura` = factura_has_producto.`factura_id_factura`
     INNER JOIN `gestiondeusuarios` gestiondeusuarios ON factura.`id_usuario` = gestiondeusuarios.`id_usuario`
     INNER JOIN `producto` producto ON factura_has_producto.`producto_id_producto` = producto.`id_producto`]]>
	</queryString>
